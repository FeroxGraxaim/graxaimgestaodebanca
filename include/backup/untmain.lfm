        Name = 'PcentLucro'
        DataType = ftString
        Precision = -1
        Size = 255
      end>
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'SELECT'
      'Times.Time,'
      'COUNT(Mercados.ROWID) AS TotalMercados,'
      'SUM(CASE WHEN Mercados.Status = ''Green'' OR Mercados.Status = ''Meio Green'' THEN 1 ELSE 0 END) AS Acertos,'
      'SUM(CASE WHEN Mercados.Status = ''Red'' OR Mercados.Status = ''Meio Red'' THEN 1 ELSE 0 END) AS Erros,'
      'SUM(CASE WHEN Mercados.Status = ''Anulada'' THEN 1 ELSE 0 END) AS Anuladas,'
      'SUM(CASE WHEN Apostas.Status = ''Green'' THEN 1 ELSE 0 END) AS Lucros,'
      'SUM(CASE WHEN Apostas.Status = ''Red'' THEN 1 END) AS Prejuízos,'
      'IFNULL((SUM(CASE WHEN Mercados.Status = ''Green'' THEN 1 ELSE 0 END) * 100.0) / COUNT(*), 0) AS PcentAcertos,'
      'IFNULL(CASE'
      'WHEN SUM(CASE WHEN Apostas.Status = ''Green'' THEN 1 ELSE 0 END) >= SUM(CASE WHEN Apostas.Status = ''Red'' THEN 1 ELSE 0 END)'
      'THEN (SUM(CASE WHEN Apostas.Status = ''Green'' THEN 1 ELSE 0 END) * 100.0) / COUNT(Mercados.ROWID)'
      'ELSE -(SUM(CASE WHEN Apostas.Status = ''Red'' THEN 1 ELSE 0 END) * 100.0) / COUNT(Apostas.ROWID)'
      'END, 0) AS PcentLucro'
      'FROM'
      'Times'
      'LEFT JOIN'
      'Jogo ON Times.Time = Jogo.Mandante OR Jogo.Visitante'
      'LEFT JOIN'
      'Mercados ON Jogo.Cod_Jogo = Mercados.Cod_Jogo'
      'LEFT JOIN'
      'Apostas ON Mercados.Cod_Aposta = Apostas.Cod_Aposta'
      'GROUP BY'
      'Times.Time'
      'ORDER BY'
      'PcentAcertos DESC, TotalMercados DESC, Time ASC;'
    )
