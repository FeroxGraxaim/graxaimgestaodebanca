      'Selecao = :Selecao,'
      'Data = :Data,'
      'Odd = :Odd,'
      'Valor_Aposta = :Valor_Aposta,'
      'Retorno = IFNULL(:Retorno, 0),'
      'Lucro = IFNULL(:Lucro, 0),'
      'Banca_Final = IFNULL(:Banca_Final, 0)'
      'WHERE Cod_Aposta = :Cod_Aposta;'
