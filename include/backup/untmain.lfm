      item
        Name = 'LucroTotalPorCento'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'MesGreen'
        DataType = ftLargeint
        Precision = -1
      end    
      item
        Name = 'MesRed'
        DataType = ftLargeint
        Precision = -1
      end>
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'SELECT'
      'Mês, Ano,'
      '(SELECT SUM(A2.Valor_Final - A2.Valor_Inicial)'
      'FROM Banca AS A2'
      'WHERE A2.Mês = Banca.Mês AND A2.Ano = Banca.Ano) AS LucroAnualReais,'
      '(SELECT SUM(A3.Valor_Inicial)'
      'FROM Banca AS A3'
      'WHERE A3.Mês = Banca.Mês AND A3.Ano = Banca.Ano) AS TotalBancas,'
      '(SELECT SUM(A2.Valor_Final - A2.Valor_Inicial)'
      'FROM Banca AS A2'
      'WHERE A2.Mês = Banca.Mês AND A2.Ano = Banca.Ano) /'
      '(SELECT SUM(CASE WHEN A3.Valor_Inicial IS NULL THEN 0 ELSE A3.Valor_Inicial END)'
      'FROM Banca AS A3'
      'WHERE A3.Mês = Banca.Mês AND A3.Ano = Banca.Ano) * 100 AS LucroTotalPorCento,'
      '(SELECT COUNT(*) FROM Apostas WHERE Apostas.Lucro > 0'
      'AND strftime(''%m'', Apostas.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")) AS MesGreen,'
      '(SELECT COUNT(*) FROM Apostas WHERE Apostas.Lucro < 0'
      'AND strftime(''%m'', Apostas.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")) AS MesRed'
      'FROM'
      'Banca'
      'WHERE'
      'Ano = (SELECT Ano FROM "Selecionar Mês e Ano");'
    )
