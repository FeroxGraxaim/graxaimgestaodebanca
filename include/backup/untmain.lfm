    SQL.Strings = (
      'SELECT'
      '    Apostas.Retorno,'
      '    Apostas.Lucro,'
      '    Apostas.Banca_Final,'
      '    Apostas.Cod_Aposta,'
      '    Apostas.Data,'
      '    Apostas.Selecao,'
      '    Apostas.Múltipla,'
      '    Apostas.Cashout,'
      '    CASE'
      '        WHEN Apostas."Múltipla" = 0 THEN Competicoes.Competicao || '': '' || Jogo.Mandante || '' X '' || Jogo.Visitante'
      '        WHEN Apostas."Múltipla" = 1 THEN ''Aposta Múltipla'''
      '    END AS Jogo,'
      '    Apostas.Odd,'
      '    Apostas.Valor_Aposta,'
      '    Apostas.Status'
      'FROM'
      '    Apostas'
      'LEFT JOIN'
      '    Mercados ON Apostas.Cod_Aposta = Mercados.Cod_Aposta'
      'LEFT JOIN'
      '    Jogo ON Mercados.Cod_Jogo = Jogo.Cod_Jogo'
      'INNER JOIN'
      '    Competicoes ON Jogo.Cod_Comp = Competicoes.Cod_Comp'
      'WHERE strftime(''%d'', Apostas.Data) BETWEEN :primDia AND :ultDia'
      'AND strftime(''%m'', Apostas.Data) BETWEEN :primMes AND :ultMes'
      'AND strftime(''%Y'', Apostas.Data) BETWEEN :primAno AND :ultAno'
      'GROUP BY Apostas.Cod_Aposta'
      'ORDER BY Apostas.Data, Apostas.Cod_Aposta;'
    )
    InsertSQL.Strings = (
      'INSERT INTO Apostas "Data", Competição_AP, Mandante, Visitante, Estratégia_Escolhida, Odd, Unidade, Valor_Aposta, Status) VALUES (:V1, V2, V3, V4, V5, V6, V7, V8, V9);'
      ''
    )
    UpdateSQL.Strings = (
