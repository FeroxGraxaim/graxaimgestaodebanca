  object qrBanca: TSQLQuery
    IndexName = 'DEFAULT_ORDER'
    MaxIndexesCount = 4
    FieldDefs = <    
      item
        Name = 'Mês'
        DataType = ftLargeint
        Precision = -1
      end    
      item
        Name = 'Ano'
        DataType = ftLargeint
        Precision = -1
      end    
      item
        Name = 'Valor_Inicial'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'Aporte'
        DataType = ftLargeint
        Precision = -1
      end    
      item
        Name = 'BancaTotal'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'LucroR$'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'Lucro_%'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'Valor_Final'
        DataType = ftFloat
        Precision = -1
      end>
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'WITH C AS ('
      'SELECT'
      '  IFNULL(B2.Mês, 0) AS Mês, IFNULL(B2.Ano, 0) AS Ano,'
      ''
      '  IFNULL((SELECT SUM (A2.Lucro) FROM Apostas A2 WHERE strftime(''%m'', A2.Data) <= B2.Mês'
      '  AND strftime(''%Y'', A2.Data = B2.Ano)), 0) AS LucroAnoAtual,'
      ''
      '  IFNULL((SELECT SUM(A3.Lucro) FROM Apostas A3 WHERE strftime(''%Y'', A3.Data) < B2.Ano), 0) AS LucroAnoPassado,'
      ''
      '  IFNULL((SELECT SUM(A6.Lucro) FROM Apostas A6 WHERE strftime(''%m'', A6.Data) < B2.Mês'
      #9#9#9#9#9#9#9#9#9#9#9#9' AND strftime(''%Y'', A6.Data) <= B2.Ano), 0) AS LucroMesPassado,'
      ''
      '  IFNULL((SELECT SUM(A5.Lucro) FROM Apostas A5 WHERE strftime(''%m'', A5.Data) = B2.Mês'
      #9#9#9#9#9#9#9#9#9#9#9'   AND strftime(''%Y'', A5.Data) = B2.Ano), 0) AS LucroMesAtual,'
      ''
      '  IFNULL((SELECT BI.Banca FROM BancaInicial BI), 0) AS BancaPrimeira,'
      ''
      '  IFNULL((SELECT SUM(B3.Aporte) FROM Banca B3 WHERE B3.Mês <= B2.Mês AND B3.Ano <= B2.Ano), 0) AS TotalAportes'
      ' FROM Banca B2'
      ' WHERE B2.Mês = :mesSelec AND B2.Ano = :anoSelec)'
      ''
      'SELECT'
      '  IFNULL(B.Mês, 0) AS Mês,'
      '  IFNULL(B.Ano, 0) AS Ano,'
      ' IFNULL((SELECT BI.Banca FROM BancaInicial BI), 0) AS Valor_Inicial,'
      '  C.TotalAportes AS Aporte,'
      ''
      '  ROUND(IFNULL(C.BancaPrimeira + C.LucroMesPassado + C.LucroAnoPassado + C.TotalAportes, 0), 2) AS BancaTotal,'
      ''
      '  ROUND(IFNULL(C.LucroMesAtual, 0), 2) AS LucroR$,'
      ''
      '  ROUND(IFNULL(C.LucroMesAtual / (C.BancaPrimeira + C.LucroMesPassado + C.LucroAnoPassado + C.TotalAportes) * 100, 0), 2) AS "Lucro_%",'
      ''
      '  ROUND(IFNULL(C.BancaPrimeira + C.LucroMesPassado + C.LucroAnoPassado + C.TotalAportes + C.LucroMesAtual, 0), 2) AS Valor_Final'
      ''
      'FROM Banca B'
      ''
      'LEFT JOIN C ON B.Mês = C.Mês AND B.Ano = C.Ano'
      ''
      'WHERE B.Mês = C.Mês'
      '  AND B.Ano = C.Ano'
      ''
      'GROUP BY B.Mês, B.Ano'
    )
    InsertSQL.Strings = (
      'INSERT INTO "Banca" ("Mês", Ano, Valor_Inicial) VALUES (:mes, :ano :novoValor)'
      ''
    )
    UpdateSQL.Strings = (
      'UPDATE "Banca Inicial" SET Valor_R$ = :novoValor WHERE "Mês" = :mesSelecionado AND Ano = :anoSelecionado'
      ''
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'mesSelec'
        ParamType = ptInput
      end    
      item
        DataType = ftUnknown
        Name = 'anoSelec'
        ParamType = ptInput
      end>
