    SQL.Strings = (
      'SELECT'
      '    Apostas.Retorno,'
      '    Apostas.Lucro,'
      '    Apostas.Banca_Final,'
      '    Apostas.Cod_Aposta,'
      '    Apostas.Data,'
      '    Apostas.Selecao,'
      '    Apostas.Múltipla,'
      '    Apostas.Cashout,'
      '    CASE'
      '        WHEN Apostas."Múltipla" = ''False'' OR Apostas."Múltipla" = 0'
      '        THEN Competicoes.Competicao || '': '' || Jogo.Mandante || '' X '' || Jogo.Visitante'
      '        WHEN ContagemJogos.Quantidade = 2 THEN ''Aposta Dupla'''
      '        WHEN ContagemJogos.Quantidade = 3 THEN ''Aposta Tripla'''
      '        WHEN ContagemJogos.Quantidade = 4 THEN ''Aposta Quádrupla'''
      '        ELSE ''Aposta Múltipla'''
      '    END AS Jogo,'
      '    Apostas.Odd,'
      '    Apostas.Valor_Aposta,'
      '    Apostas.Status'
      'FROM'
      '    Apostas'
      'LEFT JOIN'
      '    Mercados ON Apostas.Cod_Aposta = Mercados.Cod_Aposta'
      'LEFT JOIN'
      '    Jogo ON Mercados.Cod_Jogo = Jogo.Cod_Jogo'
      'INNER JOIN'
      '    Competicoes ON Jogo.Cod_Comp = Competicoes.Cod_Comp'
      'LEFT JOIN ('
      '    SELECT Cod_Aposta, COUNT(DISTINCT Cod_Jogo) AS Quantidade'
      '    FROM Mercados'
      '    GROUP BY Cod_Aposta'
      ') AS ContagemJogos ON Apostas.Cod_Aposta = ContagemJogos.Cod_Aposta'
      'WHERE strftime(''%d'', Apostas.Data) BETWEEN :primDia AND :ultDia'
      'AND strftime(''%m'', Apostas.Data) BETWEEN :primMes AND :ultMes'
      'AND strftime(''%Y'', Apostas.Data) BETWEEN :primAno AND :ultAno'
      'GROUP BY Apostas.Cod_Aposta'
      'ORDER BY Apostas.Data, Apostas.Cod_Aposta;'
      ''
      ''
    )
