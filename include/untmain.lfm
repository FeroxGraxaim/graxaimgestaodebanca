  object qrLucroAno: TSQLQuery
    FieldDefs = <>
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'WITH C AS ('
      '  SELECT'
      '    B."Mês",'
      '    B.Ano,'
      '    IFNULL((SELECT Banca FROM BancaInicial), 0) AS PrimaBanca,'
      '    IFNULL((SELECT SUM(Aporte)'
      '     FROM Banca B2'
      '     WHERE B2."Mês" <= B."Mês"'
      '     AND B2.Ano = B.Ano), 0) +'
      '    IFNULL((SELECT SUM(Aporte)'
      '     FROM Banca B3'
      '     WHERE B3.Ano < B.Ano), 0) +'
