      end    
      item
        Name = 'Lucro_%'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'Lucro%Calc'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'Valor_Final'
        DataType = ftFloat
        Precision = -1
      end    
      item
        Name = 'BancaFinalCalc'
        DataType = ftString
        Precision = -1
        Size = 255
      end    
      item
        Name = 'Stake'
        DataType = ftBCD
        Precision = 9
        Size = 2
      end    
      item
        Name = 'StakeCalc'
        DataType = ftString
        Precision = -1
        Size = 255
      end>
    Active = True
    Database = conectBancoDados
    Transaction = transactionBancoDAdos
    SQL.Strings = (
      'SELECT'
      '  Mês,'
      '  Ano,'
      '  Valor_Inicial,'
      '  ROUND(COALESCE('
      '    (SELECT SUM(Lucro)'
      '     FROM Apostas'
      '     WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '       AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0), 2) AS Lucro,'
      '  ''R$ '' || ROUND(COALESCE('
      '    (SELECT SUM(Lucro)'
      '     FROM Apostas'
      '     WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '       AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0), 2) AS LucroR$Calc,'
      '  ROUND('
      '    CASE'
      '      WHEN Valor_Inicial = 0 THEN 0'
      '      ELSE COALESCE('
      '        (SELECT SUM(Lucro)'
      '         FROM Apostas'
      '         WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '           AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0) / Valor_Inicial * 100'
      '    END, 2) AS "Lucro_%",'
      '  ROUND(COALESCE('
      '    (SELECT SUM(Lucro)'
      '     FROM Apostas'
      '     WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '       AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0), 2) || ''%'' AS "Lucro%Calc",'
      '  ROUND('
      '    CASE'
      '      WHEN COALESCE('
      '        (SELECT SUM(Lucro)'
      '         FROM Apostas'
      '         WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '           AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0) <> 0 THEN'
      '          Valor_Inicial + COALESCE('
      '            (SELECT SUM(Lucro)'
      '             FROM Apostas'
      '             WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '               AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0)'
      '      ELSE Valor_Inicial'
      '    END, 2) AS Valor_Final,'
      ''
      '    ''R$ '' || ROUND('
      '    CASE'
      '      WHEN COALESCE('
      '        (SELECT SUM(Lucro)'
      '         FROM Apostas'
      '         WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '           AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0) <> 0 THEN'
      '          Valor_Inicial + COALESCE('
      '            (SELECT SUM(Lucro)'
      '             FROM Apostas'
      '             WHERE strftime(''%m'', Apostas.Data) = Banca.Mês'
      '               AND strftime(''%Y'', Apostas.Data) = Banca.Ano), 0)'
      '      ELSE Valor_Inicial'
      '    END, 2) AS BancaFinalCalc,'
      ''
      '  Stake,'
      '  (''R$ '' || Stake) AS StakeCalc'
      'FROM'
      '  Banca;'
    )
    InsertSQL.Strings = (
      'INSERT INTO "Banca" ("Mês", Ano, Valor_Inicial, Stake) VALUES (:mes, :ano :novoValor, :stakeAposta)'
      ''
    )
    UpdateSQL.Strings = (
      'UPDATE "Banca Inicial" SET Valor_R$ = :novoValor WHERE "Mês" = :mesSelecionado AND Ano = :anoSelecionado'
      ''
    )
