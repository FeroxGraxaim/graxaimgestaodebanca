      'LEFT JOIN'
      '    Linhas ON Mercados.Cod_Linha = Linhas.Cod_Linha'
      'LEFT JOIN'
      '    Métodos ON Linhas.Cod_Metodo = Métodos.Cod_Metodo'
      'WHERE'
      '    Mercados.Cod_Aposta = :CodAposta;'
      ''
    )
