      '     WHERE A2.Mês <= (SELECT Mês FROM "Selecionar Mês e Ano")'
      '       AND A2.Ano = (SELECT Ano FROM "Selecionar Mês e Ano")) AS LucroAnualReais,'
      '    (SELECT SUM(A3.Valor_Inicial)'
      '     FROM Banca AS A3'
      '     WHERE A3.Mês <= (SELECT Mês FROM "Selecionar Mês e Ano")'
      '       AND A3.Ano = (SELECT Ano FROM "Selecionar Mês e Ano")) AS TotalBancas,'
      '    (SELECT SUM(A2.Lucro_R$)'
      '     FROM Banca AS A2'
      '     WHERE A2.Mês <= (SELECT Mês FROM "Selecionar Mês e Ano")'
      '       AND A2.Ano = (SELECT Ano FROM "Selecionar Mês e Ano")) /'
      '    (SELECT SUM(A3.Valor_Inicial)'
      '     FROM Banca AS A3'
      '     WHERE A3.Mês <= (SELECT Mês FROM "Selecionar Mês e Ano")'
      '       AND A3.Ano = (SELECT Ano FROM "Selecionar Mês e Ano")) * 100 AS LucroTotalPorCento'
      'FROM'
      '    Banca'
      'WHERE'
      '    Ano = (SELECT Ano FROM "Selecionar Mês e Ano");'
    )
