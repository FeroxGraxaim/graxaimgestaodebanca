  object qrBanca: TSQLQuery
    IndexName = 'DEFAULT_ORDER'
    MaxIndexesCount = 4
    FieldDefs = <    
      item
        Name = 'Mês'
        DataType = ftLargeint
        Precision = -1
      end    
      item
        Name = 'Ano'
