      'WITH C AS ('
      '  SELECT'
      '    B."Mês",'
      '    B.Ano,'
      '    IFNULL((SELECT Banca FROM BancaInicial), 0) +'
      '    IFNULL((SELECT SUM(Aporte)'
      '     FROM Banca B2'
      '     WHERE B2."Mês" <= B."Mês"'
      '     AND B2.Ano = B.Ano), 0) +'
      '    IFNULL((SELECT SUM(Aporte)'
      '     FROM Banca B3'
      '     WHERE B3.Ano < B.Ano), 0) +'
      '    IFNULL((SELECT SUM(Lucro)'
      '     FROM Apostas A'
      '     WHERE strftime(''%m'', A.Data) < B."Mês"'
      '     AND strftime(''%Y'', A.Data) = B.Ano), 0) +'
      '    IFNULL((SELECT SUM(Lucro)'
      '     FROM Apostas A2'
      '     WHERE strftime(''%Y'', A2.Data) < B.Ano), 0) AS BancaInicio'
      '  FROM'
      '    Banca B'
      '  LEFT JOIN "Selecionar Mês e Ano" SMI'
      '    ON SMI."Mês" = B."Mês"'
      '    AND SMI.Ano = B.Ano'
      '  WHERE'
      '    B."Mês" = SMI."Mês"'
      '    AND B.Ano = SMI.Ano'
      ')'
      'SELECT'
      '  strftime(''%d'', Apostas.Data) AS Dia,'
      ''
      '  ROUND('
      '    IFNULL(('
      '      SELECT SUM(A2.Lucro)'
      '      FROM Apostas AS A2'
      '      WHERE strftime(''%m'', A2.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '        AND strftime(''%Y'', A2.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '        AND strftime(''%d'', A2.Data) <= strftime(''%d'', Apostas.Data)'
      '    ), 0), 2) AS SomaLucro,'
      ''
      '  ROUND('
      '    CASE'
      '      WHEN C.BancaInicio = 0'
      '      THEN 0'
      '      ELSE'
      '        IFNULL(('
      '          SELECT SUM(A2.Lucro)'
      '          FROM Apostas AS A2'
      '          WHERE strftime(''%m'', A2.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '            AND strftime(''%Y'', A2.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '        '#9'AND strftime(''%d'', A2.Data) <= strftime(''%d'', Apostas.Data)), 0)'
      '        '#9'/ NULLIF(C.BancaInicio, 0) * 100'
      '    END, 2) AS PorCentoLucro,'
      ''
      '  IFNULL(('
      '    SELECT 1'
      '    FROM Apostas AS A3'
      '    WHERE strftime(''%m'', A3.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%Y'', A3.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%d'', A3.Data) = strftime(''%d'', Apostas.Data)'
      '    GROUP BY strftime(''%d'', A3.Data)'
      '    HAVING SUM(Lucro) > 0'
      '  ), 0) AS Green,'
      ''
      '  (SELECT COUNT(*)'
      '   FROM Apostas AS A5'
      '   WHERE strftime(''%m'', A5.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '     AND strftime(''%Y'', A5.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '     AND strftime(''%d'', A5.Data) = strftime(''%d'', Apostas.Data)'
      '     AND A5.Lucro > 0) AS NumGreen,'
      ''
      '  IFNULL(('
      '    SELECT 1'
      '    FROM Apostas AS A4'
      '    WHERE strftime(''%m'', A4.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%Y'', A4.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%d'', A4.Data) = strftime(''%d'', Apostas.Data)'
      '    GROUP BY strftime(''%d'', A4.Data)'
      '    HAVING SUM(Lucro) < 0'
      '  ), 0) AS Red,'
      ''
      '  IFNULL(('
      '    SELECT 1'
      '    FROM Apostas AS A4'
      '    WHERE strftime(''%m'', A4.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%Y'', A4.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '      AND strftime(''%d'', A4.Data) = strftime(''%d'', Apostas.Data)'
      '      AND A4.Status <> ''Pré-live'''
      '    GROUP BY strftime(''%d'', A4.Data)'
      '    HAVING SUM(Lucro) = 0'
      '  ), 0) AS Neutro,'
      ''
      '  (SELECT COUNT(*)'
      '   FROM Apostas AS A5'
      '   WHERE strftime(''%m'', A5.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '     AND strftime(''%Y'', A5.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      '     AND strftime(''%d'', A5.Data) = strftime(''%d'', Apostas.Data)'
      '     AND A5.Lucro < 0) AS NumRed'
      ''
      'FROM'
      '  Apostas'
      'LEFT JOIN'
      '  Banca ON Banca."Mês" = strftime(''%m'', Apostas.Data)'
      '          AND Banca.Ano = strftime(''%Y'', Apostas.Data)'
      'LEFT JOIN'
      '  C ON C."Mês" = strftime(''%m'', Apostas.Data)'
      '      AND C.Ano = strftime(''%Y'', Apostas.Data)'
      'WHERE'
      '  strftime(''%m'', Apostas.Data) = (SELECT Mês FROM "Selecionar Mês e Ano")'
      '  AND strftime(''%Y'', Apostas.Data) = (SELECT Ano FROM "Selecionar Mês e Ano")'
      'GROUP BY'
      '  Dia'
      'ORDER BY'
      '  Dia;'
    )
